{
  "name": "fisdascms",
  "version": "1.2.0",
  "author": "Izzu Zantya Fawwas",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "install": "pnpm install:server && pnpm install:client",
    "install:server": "cd apps/fisdascms-server && pnpm install",
    "install:client": "cd apps/fisdascms-client && pnpm install",
    "dev:server": "cd apps/fisdascms-server && pnpm dev",
    "dev:client": "cd apps/fisdascms-client && pnpm dev",
    "test:server": "cd apps/fisdascms-server && pnpm test",
    "build": "pnpm build:server && pnpm build:client",
    "build:server": "cd apps/fisdascms-server && pnpm build",
    "build:client": "cd apps/fisdascms-client && pnpm build",
    "start:server": "cd apps/fisdascms-server && pnpm start:prod",
    "start:client": "cd apps/fisdascms-client && pnpm start",
    "heroku-postbuild": "pnpm run build:server"
  }
}
