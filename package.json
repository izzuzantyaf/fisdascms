{
  "name": "fisdascms",
  "version": "1.1.0",
  "author": "Izzu Zantya Fawwas",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "install": "yarn install:server && yarn install:client",
    "install:server": "cd apps/fisdascms-server && yarn install",
    "install:client": "cd apps/fisdascms-client && yarn install",
    "dev:server": "cd apps/fisdascms-server && yarn dev",
    "dev:client": "cd apps/fisdascms-client && yarn dev",
    "test:server": "cd apps/fisdascms-server && yarn test",
    "build": "yarn build:server && yarn build:client",
    "build:server": "cd apps/fisdascms-server && yarn build",
    "build:client": "cd apps/fisdascms-client && yarn build",
    "start:server": "cd apps/fisdascms-server && yarn start:prod",
    "start:client": "cd apps/fisdascms-client && yarn start",
    "heroku-postbuild": "yarn run build:server"
  }
}
