{
  "name": "fisdascms",
  "version": "1.3.0",
  "author": "Izzu Zantya Fawwas",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "install": "pnpm install:server && pnpm install:client",
    "install:server": "cd apps/fisdascms-server && pnpm install",
    "install:client": "cd apps/fisdascms-client && pnpm install",
    "dev:server": "cd apps/fisdascms-server && pnpm dev",
    "dev:client": "cd apps/fisdascms-client && pnpm dev",
    "test:server": "cd apps/fisdascms-server && pnpm test",
    "build": "yarn build:server && yarn build:client",
    "build:server": "cd apps/fisdascms-server && yarn build",
    "build:client": "cd apps/fisdascms-client && yarn build",
    "start:server": "cd apps/fisdascms-server && yarn start:prod",
    "start:client": "cd apps/fisdascms-client && yarn start",
    "heroku-postbuild": "yarn run build:server"
  }
}
